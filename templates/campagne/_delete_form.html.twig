<form method="post" action="{{ path('campagne_delete', {'id': campagne.id}) }}" onsubmit="return confirm('Etes-vous sur de vouloir supprimer cet objet?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ campagne.id) }}">
    <button class="btn">Supprimer</button>
</form>
